<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace = "com.jinanging.spring.test.thymeleaf.repository.StoreReviewRepository">
 
 	<select id = "selectStore"
 			resultType = "com.jinanging.spring.test.thymeleaf.domain.StoreReview">
 			
 			SELECT 
 				`id`
 				, `name`
 				, `phoneNumber`
 				, `address`
 				, `businessNumber`
 				, `introduce`
 				, `createdAt`
 				, `updatedAt`
 			FROM
 				`store`
 	</select>
 	
 	
 </mapper>