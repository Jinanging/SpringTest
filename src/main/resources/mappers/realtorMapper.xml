<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace = "com.jinanging.spring.test.mvc.respository.RealtorRepository">
 
 <insert id ="insertRealtor" useGeneratedKeys="true" keyProperty="id" 
 parameterType = "com.jinanging.spring.test.mvc.domain.Realtor">
 	INSERT INTO
 		`realtor`
 	(
 		`office`
 		, `phoneNumber`
 		, `address`
 		, `grade`
 		
 	)
 	VALUE
 	(
 		#{office}
 		, #{phoneNumber}
 		, #{address}
 		, #{grade}
 	)
 		
 </insert>
 
 
 
 
 </mapper>
 